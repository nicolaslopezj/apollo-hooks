{
  "name": "apollo-hooks",
  "version": "5.0.6",
  "description": "Hooks for Apollo",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/apollo-hooks.umd.js",
  "module": "./dist/apollo-hooks.es.js",
  "exports": {
    ".": {
      "import": "./dist/apollo-hooks.es.js",
      "require": "./dist/apollo-hooks.umd.js"
    }
  },
  "repository": "https://github.com/nicolaslopezj/apollo-hooks",
  "author": "nicolaslopezj",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "exit 0",
    "prepare": "yarn run build",
    "clean": "rm -rf ./dist",
    "dev": "watch -p './src/**/*.ts' -c 'yarn build'",
    "build": "vite build && yarn emit-types",
    "emit-types": "tsc"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "object-hash": "3.0.0",
    "react-fast-compare": "^3.2.0"
  },
  "peerDependencies": {
    "@apollo/client": "*",
    "graphql": "^16.5.0",
    "react": "^16.7.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.5",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.182",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^18.0.9",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "eslint-config-orionsoft": "^2.2.0",
    "graphql": "^16.5.0",
    "react": "^17.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "watch-cli": "^0.2.3"
  }
}
