{
  "name": "apollo-hooks",
  "version": "6.0.5",
  "description": "Hooks for Apollo",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/apollo-hooks.umd.js",
  "module": "./dist/apollo-hooks.es.js",
  "exports": {
    ".": {
      "import": "./dist/apollo-hooks.es.js",
      "require": "./dist/apollo-hooks.umd.js"
    }
  },
  "repository": "https://github.com/nicolaslopezj/apollo-hooks",
  "author": "nicolaslopezj",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "exit 0",
    "prepare": "yarn run build",
    "clean": "rm -rf ./dist",
    "dev": "watch -p './src/**/*.ts' -c 'yarn build'",
    "build": "vite build && yarn emit-types",
    "emit-types": "tsc"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "object-hash": "3.0.0",
    "react-fast-compare": "^3.2.2"
  },
  "peerDependencies": {
    "@apollo/client": "*",
    "graphql": "*",
    "react": "*"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.17",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.2.14",
    "graphql": "^16.7.1",
    "react": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "watch-cli": "^0.2.3"
  }
}
