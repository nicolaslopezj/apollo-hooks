{
  "name": "apollo-hooks",
  "version": "5.0.0",
  "description": "Hooks for Apollo",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/apollo-hooks.umd.js",
  "module": "./dist/apollo-hooks.es.js",
  "exports": {
    ".": {
      "import": "./dist/apollo-hooks.es.js",
      "require": "./dist/apollo-hooks.umd.js"
    }
  },
  "repository": "https://github.com/nicolaslopezj/apollo-hooks",
  "author": "nicolaslopezj",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "exit 0",
    "prepare": "yarn run build",
    "clean": "rm -rf ./dist",
    "dev": "vite",
    "build": "vite build && yarn emit-types",
    "emit-types": "tsc --declaration --emitDeclarationOnly --jsx react --esModuleInterop --outDir dist"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react-fast-compare": "^3.2.0"
  },
  "peerDependencies": {
    "@apollo/client": "*",
    "react": "^16.7.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.6",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "eslint-config-orionsoft": "^2.2.0",
    "graphql": "^16.1.0",
    "react": "^17.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9"
  }
}
